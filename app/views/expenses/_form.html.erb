	<%= form_for @expense do |f| %>
	<% if@expense.errors.any? %>
	<div id='error explanation'>
		<h2><%= pluralize(@expense.errors.count,'error')%>Prohibited expense from being saved:</h2>
		<ul>
			<% @expense.errors.full_messages.each do |msg| %>
			<li><%= msg %></li>
			<% end %>
		</ul>
	</div>
	<% end %>
	<p>
		<%= f.label :Accomidation %>
		<%= f.text_field :accomidation %>
	</p>
	<p>
		<%= f.label :Travelling %>
		<%= f.text_field :travelling %>
	</p>
	<p>
		<%= f.label :Food %>
		<%= f.text_field :food %>
	</p>
	<p>
		<%= f.label :Othercharges %>
		<%= f.text_field :othercharges %>
	</p>
	<p>
	     <%= f.submit :submit %>
	</p>
	<% end %>

